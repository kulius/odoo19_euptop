<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Employee Number Sequence (備用，目前員工編號由程式自動產生) -->
        <record id="seq_sl_employee_number" model="ir.sequence">
            <field name="name">Employee Number Sequence</field>
            <field name="code">sl.employee.number</field>
            <field name="prefix">%(y)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
